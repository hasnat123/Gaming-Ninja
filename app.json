{
  "expo": {
    "name": "Gaming Ninja",
    "slug": "gaming-ninja",
    "description": "A video game discovery app created using React Native and the RAWG API",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ff1d2d35-a715-44d6-9793-aab75843b431"
      }
    },
    "owner": "hasnat123",
    "updates": {
      "url": "https://u.expo.dev/ff1d2d35-a715-44d6-9793-aab75843b431"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
